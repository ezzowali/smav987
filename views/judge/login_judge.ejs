              <section id="registerForm">

        <div class="container ">

            <div class="card-body content">
                <br><br>
                <% if (message) { %>
                    <div class="col-lg-6 col-md-9 alert alert-danger alert-dismissible fade show">
                        <strong>!!خطأ</strong> الايميل مستخدم
                        <button type="button" class="close" data-dismiss="alert">&times;</button>
                    </div>
                    <% } %>
                        <% if (message2) { %>
                            <div class="col-lg-6 col-md-9 alert alert-success alert-dismissible fade show">
                                تم تسجيلك
                                <a href="/"> للعودة من هنا </a>
                                <button type="button" class="close" data-dismiss="alert">&times;</button>
                            </div>
                            <% } %>

                            <form action="/login_judge" method="POST" >
                                    <div class="row">



                                        <div class="col-lg-6 col-md-9">
                                            <br><br>
                                            <br><br>

                                            <div class="frontside">
                                                <div class="card ">
                                                    <div class="card-body text-center">
                                                        <h4 class="card-title "> <i class="fas fa-user-circle"></i> المعلومات الشخصية </h4>

                                                       

                                             


                                                      <br>
                                                      <br>
                                                      <br>
                                                 

                                                        <div class="row">

                                                            <div class="col-md-9 mb-3">

                                                                <input type="email" class="col-md-12 effect-16 inputs" name="email" value="" placeholder="البريد الالكتروني   " required>
                                                                <span class="focus-border"></span>
                                                            </div>
                                                        </div><br>
                                                      <br>


                                                        <div class="row">

                                                          <div class="col-md-9 mb-3">

                                                            <input name="password" class="col-md-12 effect-16 inputs" placeholder="******" type="password">
                                                            <span class="focus-border"></span>
                                                          </div>
                                                      </div>
                                                      <br>
                                                      <br>
                                                      <br>
                                                      <br>
                                                   





                                                   





                                               



                                    

                                                           


                                                                <input type="hidden" name="_csrf" value="<%= csrfToken %>">
                                                                <button type="submit" class="btn btn-info">تسجيل</button>
                                                            </div>
                                                        </div>


                                                    </div>



                                                </div>


                                            </div>

                                        </div>

